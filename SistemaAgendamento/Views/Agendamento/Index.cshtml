<div class="envelope-pesquisa">
    <h3 class="titulopagina">Agendamento</h3>
</div>
<div class="container">
    <div id="TabelaAgendamento" class="row">
        <table class="table table-striped .table-responsive">
            <thead>
                <tr>
                    <th>Data</th>
                    <th>Cliente</th>
                    <th>Categoria</th>
                    <th>Serviço</th>
                    <th>Colaborador</th>
                    <th>Ações</th>
                </tr>
            </thead>
            <tbody id="CorpoTabelaAgendamento"></tbody>
        </table>
    </div>
</div>
<script type="text/x-handlebars" id="TemplateTabelaAgendamentos">
    {{#Agendamento}}
    <tr>
        <td class="hidden" id="{{a009_id}}"></td>
        <td>{{a009_dataDT}}</td>
        <td>{{a004_nomecliente}}</td>
        <td>{{a005_nomecategoria}}</td>
        <td>{{a006_nomeservico}}</td>
        <td>{{a003_nomecolaborador}}</td>
        <td>
            <span name="EditarAgendamento" class="btn btn-xs btn-warning">
                <i class="fa fa-pencil"></i>
            </span>
            <span name="ExcluirAgendamento" class="btn btn-xs btn-danger">
                <i class="fa fa-times"></i>
            </span>
        </td>
    </tr>
    {{/Agendamento}}
    {{^Agendamento}}
    <h3>Não existem agendamentos gerados!</h3>
    {{/Agendamento}}
</script>
<script src="~/Scripts/Site/Agendamento/Index.js" type="text/javascript"></script>