<div class="modal-header">
    <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
    <h4 class="modal-title" id="myModalLabel"></h4>
</div>
<div class="modal-body">
    <form id="FormAdicionaCliente">
        <div class="form-group">
            <label>Categoria</label>
            <select name="CategoriaServico" class="form-control">
                <option hidden disabled selected value="">Selecione uma opção</option>
            </select>
        </div>
        <div class="form-group">
            <label>Servico</label>
            <select name="Servicos" class="form-control">
                <option hidden disabled selected value="">Selecione uma opção</option>
            </select>
        </div>
        <div class="form-group">
            <label>Colaborador</label>
            <select name="Colaborador" class="form-control">
                <option hidden disabled selected value="">Selecione uma opção</option>
            </select>
        </div>
        <div class="form-group">
            <label>Data</label>
            <input name="Data" type="date" data-mask="00/00/0000" class="form-control" />
        </div>
        <button class="hidden" name="SalvarAgendamento" type="submit"></button>
    </form>
</div>
<div class="modal-footer">
    <button type="button" class="btn btn-danger" data-dismiss="modal">Cancelar</button>
    <button name="BtnAgendaCliente" class="btn btn-success">
        <i class="fa fa-check"></i>Agendar
    </button>
</div>

